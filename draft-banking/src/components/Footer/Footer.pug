- var links = ['Team', 'Press', 'Fees', 'Services', 'Projects', 'Affiliate', 'Terms of use', 'Privacy policy', 'Contact us']

footer.footer
	.footer__content.container
		.footer__copyright
			img(src="img/logo.svg" alt="Page logo")
			p.text.footer__text Â© 2020 Page example
		.footer__links
			ul
				each link in links
					li
						a.text.footer__text(href="#")= link